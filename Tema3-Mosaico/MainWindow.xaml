<Window x:Class="Tema3_Mosaico.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3_Mosaico"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="500" MinHeight="400" MinWidth="400">
    <DockPanel>
        <TextBlock DockPanel.Dock="Top" Height="100" Background="#FF9421F3"></TextBlock>
        <TextBlock DockPanel.Dock="Left" Width="100" Background="#FF9421F3"></TextBlock>
        <TextBlock DockPanel.Dock="Bottom" Height="100" Background="#FF35741E"></TextBlock>
        <TextBlock DockPanel.Dock="Right" Width="100" Background="#FF35741E"></TextBlock>
        <DockPanel>
            <TextBlock DockPanel.Dock="Bottom" Height="100" Background="#FFC35F0C"></TextBlock>
            <TextBlock DockPanel.Dock="Left" Width="100" Background="#FFC35F0C"></TextBlock>
            <TextBlock DockPanel.Dock="Right" Width="100" Background="#FFE61A93"></TextBlock>
            <TextBlock DockPanel.Dock="Top" Height="100" Background="#FFE61A93"></TextBlock>
            <TextBlock Background="Black"></TextBlock>
        </DockPanel>
    </DockPanel>
</Window>
